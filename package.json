    "prettier": "^3.4.2",
    "slugify": "^1.6.6",
    "tsx": "4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.7.3",
    "undici": "^7.20.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
