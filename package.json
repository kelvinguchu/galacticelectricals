    "@testing-library/react": "16.3.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.11",
    "fast-xml-parser": "^5.3.4",
