    "@payloadcms/next": "3.75.0",
    "@payloadcms/richtext-lexical": "3.75.0",
    "@payloadcms/storage-uploadthing": "^3.75.0",
    "@payloadcms/ui": "3.75.0",
    "@react-email/components": "^1.0.7",
    "@react-email/render": "^2.0.4",
    "@react-pdf/renderer": "^4.3.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@upstash/redis": "^1.36.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.7",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.8.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.563.0",
    "motion": "^12.34.0",
    "next": "15.4.11",
    "next-themes": "^0.4.6",
    "payload": "3.75.0",
    "postcss": "^8.5.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.1",
    "react-email": "^5.2.8",
    "react-hook-form": "^7.71.1",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^4.6.0",
    "recharts": "2.15.4",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vaul": "^1.1.2",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
